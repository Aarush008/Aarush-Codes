#Fibonacci Sequence
t=int(input("Enter the number of terms: "))
n1=0
n2=1
count=0
if t<=0:
    print("Please enter a positive value")
elif t==1:
    print("Fibonacci Sequence: \n1")
else:
    print("Fibonacci Sequence: ")
    while count<t:
        print(n1)
        n3=n1+n2
        n1=n2
        n2=n3
        count+=1
